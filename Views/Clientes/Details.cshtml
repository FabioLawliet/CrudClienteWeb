@model CrudClienteWeb.Models.DtoCliente

@{
    ViewData["Title"] = "Details";
}

<h1>CLIENTE</h1>
<h3>Detalhes</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nome
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nome)
        </dd>
        <dt class = "col-sm-2">
            CPF/CNPJ
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.cpfcnpj)
        </dd>
        <dt class = "col-sm-2">
            RG/IE
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.rgie)
        </dd>
        <dt class = "col-sm-2">
            Ativo?
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ativo)
        </dd>
        <dt class = "col-sm-2">
            Endereço
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.endereco)
        </dd>
        <dt class = "col-sm-2">
            Número
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.numero)
        </dd>
        <dt class = "col-sm-2">
            Bairro
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.bairro)
        </dd>
        <dt class = "col-sm-2">
            Complemento
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.complemento)
        </dd>
        <dt class = "col-sm-2">
            CEP
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.cep)
        </dd>
        <dt class = "col-sm-2">
            Cidade
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomecidade)
        </dd>
        <dt class = "col-sm-2">
            Estado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.siglaestado)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.id" class="btn btnCad btn-primary">Editar</a>
    <a asp-action="Index" class="btn btnCad btn-primary">Voltar</a>
</div>
